{
  "name": "lanius/jobman",
  "type": "typo3-cms-extension",
  "description": "Job listings with Google for Jobs microdata and tt_address support, featuring flexible display options such as accordions, tiles, and detailed job views.",
  "license": "GPL-2.0-or-later",
  "version": "1.3.4",
  "authors": [
    {
      "name": "Andre Lanius",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^13.4 || ^14.0 || ^14.1",
    "friendsoftypo3/tt-address": "*",
    "php": "^8.1 || ^8.2 || ^8.3 || ^8.4 || ^8.5"
  },
  "autoload": {
    "psr-4": {
      "Lanius\\Jobman\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "jobman"
    }
  }
}
